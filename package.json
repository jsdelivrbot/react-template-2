{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Basic template for React app with support for Babel",
  "main": "index.js",
  "repository": "git@github.com:https://github.com/wakorb/react-template.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --require babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage",
    "format": "prettier --write '**/*.{js,jsx}'",
    "lint": "eslint '**/*.{js,jsx}' --quiet"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "chai-jquery": "^2.0.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.62.0",
    "jquery": "3.2.1",
    "jsdom": "11.5.1",
    "mocha": "4.1.0",
    "prettier": "^1.9.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.24.1",
    "lodash": "4.17.4",
    "prop-types": "^15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "3.7.2"
  }
}
